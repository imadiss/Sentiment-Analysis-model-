{
  "builds": [{
      "src": "sentiment_analysis/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
  }],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "sentiment_analysis/wsgi.py"
      }
  ],
  "functions": {
    "api/**": {
      "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
    }
  }
}